import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.services.extended.*;

public class FAASListUpdateService {

    @DataContext('faas_list')
    def em_faas_list


   @ProxyMethod
   public void update(param) {
        def faaslist = em_faas_list.findFaasListForUpdate(param)
        em_faas_list.find([objid: param.objid]).update(faaslist)
   }
}